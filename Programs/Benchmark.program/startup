local function t(e,o,a)
if not fs.exists("/"..(a and e..".lua"or e))and not _G[e]then
printError("No "..e.." API found, downloading one...")
shell.run("pastebin get "..o.." /"..(a and e..".lua"or e))
error()
end
return true
end
